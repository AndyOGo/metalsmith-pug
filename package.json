{
  "version": "2.0.0",
  "name": "metalsmith-jade",
  "description": "Metalsmith plugin to convert jade files.",
  "author": "Ahmad Nassri <ahmad@ahmadnassri.com> (https://www.ahmadnassri.com/)",
  "repository": "ahmadnassri/metalsmith-jade",
  "license": "MIT",
  "main": "src/index.js",
  "keywords": [
    "metalsmith",
    "plugin",
    "jade"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "files": [
    "src"
  ],
  "bugs": {
    "url": "https://github.com/ahmadnassri/metalsmith-jade/issues"
  },
  "scripts": {
    "pretest": "standard && echint",
    "test": "mocha",
    "posttest": "npm run coverage",
    "coverage": "istanbul cover --dir coverage _mocha -- -R dot",
    "codeclimate": "codeclimate < coverage/lcov.info"
  },
  "echint": {
    "ignore": [
      "coverage/**"
    ]
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.0.4",
    "del": "^1.2.0",
    "echint": "^1.1.1",
    "istanbul": "^0.3.14",
    "metalsmith": "^1.7.0",
    "mocha": "^2.2.5",
    "should": "^6.0.3",
    "standard": "^3.11.1"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "jade": "^1.10.0"
  }
}
